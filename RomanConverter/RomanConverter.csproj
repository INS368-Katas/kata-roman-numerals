<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>KataRomanConverter</PackageId>
    <Version>1.0.0</Version>
    <Authors>Grupo 3</Authors>
    <Company>INS368-Katas</Company>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\$(AssemblyName).xml</DocumentationFile>
    <PackageDescription>Este packete genera una referencia a una clase con dos metodos que se encargan de convertir numeros del sistema numerico arabigo a el sistema numerico romano y vice versa.</PackageDescription>
  </PropertyGroup>



</Project>
